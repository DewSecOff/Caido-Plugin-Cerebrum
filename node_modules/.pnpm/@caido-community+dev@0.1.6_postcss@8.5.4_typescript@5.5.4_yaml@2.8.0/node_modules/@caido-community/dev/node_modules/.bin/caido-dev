#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dewsecoff/Documents/Caido/Plugin/Organizer/node_modules/.pnpm/@caido-community+dev@0.1.6_postcss@8.5.4_typescript@5.5.4_yaml@2.8.0/node_modules/@caido-community/dev/dist/node_modules:/home/dewsecoff/Documents/Caido/Plugin/Organizer/node_modules/.pnpm/@caido-community+dev@0.1.6_postcss@8.5.4_typescript@5.5.4_yaml@2.8.0/node_modules/@caido-community/dev/node_modules:/home/dewsecoff/Documents/Caido/Plugin/Organizer/node_modules/.pnpm/@caido-community+dev@0.1.6_postcss@8.5.4_typescript@5.5.4_yaml@2.8.0/node_modules/@caido-community/node_modules:/home/dewsecoff/Documents/Caido/Plugin/Organizer/node_modules/.pnpm/@caido-community+dev@0.1.6_postcss@8.5.4_typescript@5.5.4_yaml@2.8.0/node_modules:/home/dewsecoff/Documents/Caido/Plugin/Organizer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dewsecoff/Documents/Caido/Plugin/Organizer/node_modules/.pnpm/@caido-community+dev@0.1.6_postcss@8.5.4_typescript@5.5.4_yaml@2.8.0/node_modules/@caido-community/dev/dist/node_modules:/home/dewsecoff/Documents/Caido/Plugin/Organizer/node_modules/.pnpm/@caido-community+dev@0.1.6_postcss@8.5.4_typescript@5.5.4_yaml@2.8.0/node_modules/@caido-community/dev/node_modules:/home/dewsecoff/Documents/Caido/Plugin/Organizer/node_modules/.pnpm/@caido-community+dev@0.1.6_postcss@8.5.4_typescript@5.5.4_yaml@2.8.0/node_modules/@caido-community/node_modules:/home/dewsecoff/Documents/Caido/Plugin/Organizer/node_modules/.pnpm/@caido-community+dev@0.1.6_postcss@8.5.4_typescript@5.5.4_yaml@2.8.0/node_modules:/home/dewsecoff/Documents/Caido/Plugin/Organizer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
